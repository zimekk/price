  "dependencies": {
    "@acme/components": "workspace:*",
    "@acme/prod": "workspace:*",
    "@acme/sale": "workspace:*",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.7",
    "rxjs": "^7.8.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "latest",
    "eslint": "^9.0.0",
    "typescript": "5.5.4"
  }
