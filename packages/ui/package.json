{
  "name": "@acme/ui",
  "version": "0.0.1",
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf .turbo"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./*": [
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts"
    ],
    "./tailwind": {
      "require": "./src/tailwind.cjs"
    }
  },
  "files": [
    "src"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "peerDependencies": {
    "next": "^15.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.17",
    "@vercel/examples-ui": "^1.0.4",
    "clsx": "^2.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.0",
    "eslint": "^9.37.0",
    "typescript": "5.8.3"
  }
}
